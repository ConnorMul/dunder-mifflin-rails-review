<h1>New Employee</h1>

<%= form_for @employee do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <br>
  <br>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <br>
  <br>
  <%= f.label :alias %>
  <%= f.text_field :alias %>
  <br>
  <br>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <br>
  <%= f.label :office %>
  <%= f.text_field :office %>
  <br>
  <br>
  <%= f.label :dog_id %>
  <%= f.collection_select :dog_id, Dog.all, :id, :name %>
  <%= f.submit %>
<% end %>




